    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "outDir": "./lib",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/**/*"],
  "exclude": ["node_modules"]
}